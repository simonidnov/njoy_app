<?xml version='1.0' encoding='utf-8'?>
<widget id="com.njoy.regis" version="1.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NJOY</name>
    <description>
        NJOY APP
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Simon DELAMARRE
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="27" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <config-file parent="UISupportedInterfaceOrientations" platform="ios" target="*-Info.plist">
            <array>
                <string>UIInterfaceOrientationPortrait</string>
                <string>UIInterfaceOrientationPortraitUpsideDown</string>
            </array>
        </config-file>
        <config-file parent="UISupportedInterfaceOrientations~ipad" platform="ios" target="*-Info.plist">
            <array>
                <string>UIInterfaceOrientationLandscapeLeft</string>
                <string>UIInterfaceOrientationLandscapeRight</string>
            </array>
        </config-file>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <preference name="Orientation" value="landscape" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <splash density="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
        <splash density="land-ldpi" src="res/screen/android/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" src="res/screen/android/screen-mdpi-landscape.png" />
        <splash density="land-xhdpi" src="res/screen/android/screen-xhdpi-landscape.png" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
        <splash density="port-mdpi" src="res/screen/android/screen-mdpi-portrait.png" />
        <splash density="port-xhdpi" src="res/screen/android/screen-xhdpi-portrait.png" />
    </platform>
    <platform name="ios">
        <icon height="29" src="res/icons/Icon-Small.png" width="29" />
        <icon height="58" src="res/icons/Icon-Small@2x.png" width="58" />
        <icon height="87" src="res/icons/Icon-Small@3x.png" width="87" />
        <icon height="40" src="res/icons/Icon-40.png" width="40" />
        <icon height="80" src="res/icons/Icon-40@2x.png" width="80" />
        <icon height="60" src="res/icons/Icon-60.png" width="60" />
        <icon height="120" src="res/icons/Icon-60@2x.png" width="120" />
        <icon height="180" src="res/icons/Icon-60@3x.png" width="180" />
        <icon height="57" src="res/icons/Icon.png" width="57" />
        <icon height="114" src="res/icons/Icon@2x.png" width="114" />
        <icon height="50" src="res/icons/Icon-50.png" width="50" />
        <icon height="100" src="res/icons/Icon-50@2x.png" width="100" />
        <icon height="76" src="res/icons/Icon-76.png" width="76" />
        <icon height="152" src="res/icons/Icon-76@2x.png" width="152" />
        <icon height="72" src="res/icons/Icon-72.png" width="72" />
        <icon height="144" src="res/icons/Icon-72@2x.png" width="144" />
        <icon height="167" src="res/icons/Icon-83.5@2x.png" width="167" />
        <splash height="480" platform="ios" src="res/splash/Default.png" width="320" />
        <splash height="960" platform="ios" src="res/splash/Default@2x.png" width="640" />
        <splash height="1136" platform="ios" src="res/splash/Default-568h@2x.png" width="640" />
        <splash height="1334" platform="ios" src="res/splash/Default-667h@2x.png" width="750" />
        <splash height="2208" platform="ios" src="res/splash/Default-Portrait-736h@3x.png" width="1242" />
        <splash height="1242" platform="ios" src="res/splash/Default-Landscape-736h@3x.png" width="2208" />
        <splash height="1024" platform="ios" src="res/splash/Default-Portrait.png" width="768" />
        <splash height="768" platform="ios" src="res/splash/Default-Landscape.png" width="1024" />
        <splash height="2048" platform="ios" src="res/splash/Default-Portrait@2x.png" width="1536" />
        <splash height="1536" platform="ios" src="res/splash/Default-Landscape@2x.png" width="2048" />
    </platform>
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-battery-status" spec="^1.2.4" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-open-native-settings" spec="^1.5.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.0.1">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <engine name="ios" spec="^4.4.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
